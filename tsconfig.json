{
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "lib": [
      "esnext",
      "DOM"
    ],
    "outDir": "./lib",
    "rootDir": "./src",
    "strict": true,
    "noUncheckedIndexedAccess": true
  }
}
